<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp1.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    FlyoutIcon="home.png"
    xmlns:local="clr-namespace:MauiApp1">

    <FlyoutItem Title="Menu prueba" FlyoutDisplayOptions="AsMultipleItems">
        <ShellContent Icon="dotnet_bot.png"
            Title="Home"
             ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage" />

        <ShellContent Icon="perro1.png"
            Title="Perros"
            ContentTemplate="{DataTemplate local:Perros}"
            Route="Perros" />

        <ShellContent Icon="gato1.jpg"
            Title="Gatos"
            ContentTemplate="{DataTemplate local:Gatos}"
            Route="Gatos" />

        <ShellContent Icon="hamster1.jpg"
            Title="Hamsters"
            ContentTemplate="{DataTemplate local:Hamsters}"
            Route="Hamsters" />

    </FlyoutItem>
</Shell>